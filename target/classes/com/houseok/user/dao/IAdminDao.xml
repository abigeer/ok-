<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.houseok.user.dao.IAdminDao">

	<select id="selectAdminatorByAccount" resultType="Adminator">
		select * from tb_adminator where account=#{account}
	</select>
	

	<delete id="deleteUserById">
		delete from tb_user where id=#{id}
	</delete>
	
	
</mapper>